{
  "name": "dogestore",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node -r dotenv/config ./build/index.js",
    "dev": "concurrently \"npm:watch\" \"npm:serve\"",
    "serve": "node -r dotenv/config ./build/index.js",
    "serve_nodemom": "nodemon -r dotenv/config ./build/index.js",
    "build": "tsc && npm run copy-files",
    "watch": "tsc -w",
    "test_and_coverage": "jest --coverage --collectCoverageFrom='./src/**'",
    "test": "jest",
    "copy-files": "copyfiles -u 1 src/**/*.sql build/",
    "init_db": "npm run build && node ./build/db/seed.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "copyfiles": "^2.4.1",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.21",
    "@types/uuid": "^8.3.4",
    "bcrypt": "^5.0.1",
    "concurrently": "^7.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.1",
    "express-validation": "^4.1.0",
    "formidable": "^3.2.4",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.20.0",
    "uuid": "^8.3.2"
  }
}
